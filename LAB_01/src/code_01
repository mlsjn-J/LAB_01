#include <stdio.h>

void main()
{
    int h, m;
    printf("시와 분을 입력하세요 : "); scanf("%d %d", &h, &m);

    if(h==0)
        if(m>=45)
            printf("00 %d", m-45);
        else
            printf("23 %d", m+15);
    else
        if(m>=45)
            printf("%d %d", h, m-45);
        else
            printf("%d %d", h-1, m+15);
}
